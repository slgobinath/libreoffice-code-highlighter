  <dependencies>
    <l:LibreOffice-minimal-version value="4.0" d:name="LibreOffice.org or Open Office 4.0" />
  </dependencies>
  <registration>
    <simple-license accept-by="admin">
      <license-text xlink:href="license/license.txt" />
      <license-text xlink:href="license/license.txt" lang="en-US" />
